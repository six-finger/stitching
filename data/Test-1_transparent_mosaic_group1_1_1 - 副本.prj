PROJCS[
    "WGS 84 / UTM zone 51N",                    # 此投影坐标系是基于WGS 84椭球体的UTM投影，其中"zone 51N"表示北半球的第51个UTM投影带，适用于经度范围117度至123度之间的区域
    GEOGCS[
        "WGS 84",                               # 地理坐标系：WGS 84
        DATUM[
            "WGS_1984", 
            SPHEROID["WGS 84", 6378137, 298.257223563, AUTHORITY["EPSG","7030"]],       # 基准椭球体 长轴 偏心率0.298257...
            AUTHORITY["EPSG","6326"]
        ],
        PRIMEM["Greenwich", 0, AUTHORITY["EPSG","8901"]],                       # 本初子午线，即参考经线的位置
        UNIT["degree", 0.0174532925199433, AUTHORITY["EPSG","9122"]],           # 将度转换为弧度的乘法因子
        AUTHORITY["EPSG","4326"]
    ],
    PROJECTION["Transverse_Mercator"],          # 投影方法：横轴墨卡托投影
    PARAMETER["latitude_of_origin",0],          # 原点纬度
    PARAMETER["central_meridian",123],          # 中央子午线经度
    PARAMETER["scale_factor",0.9996],           # 比例因子
    PARAMETER["false_easting",500000],          # 假东移
    PARAMETER["false_northing",0],              # 假北移
    UNIT["metre",1,AUTHORITY["EPSG","9001"]],   # 单位：米
    AXIS["Easting",EAST],                       # 东 X
    AXIS["Northing",NORTH],                     # 北 Y
    AUTHORITY["EPSG","32651"]
]

